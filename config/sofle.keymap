    sensors: sensors {
        compatible = "zmk,keymap-sensors";
        sensors = <&left_encoder &right_encoder>;
        triggers-per-rotation = <30>;
    };

    keymap {
        compatible = "zmk,keymap";

        QWERTZ_Layer {
            display-name = "qwertz";
            bindings = <
&kp ESC         &kp N1               &kp N2           &kp N3    &kp N4             &kp N5                                             &kp N6                  &kp N7        &kp N8        &kp N9           &kp N0                         &kp DE_SZ
&kp TAB         &kp Q                &kp W            &kp E     &kp R              &kp T                                              &mod_taptap MINUS DE_Z  &kp U         &kp I         &kp O            &kp P                          &kp DE_U_UMLAUT
&kp LEFT_SHIFT  &shml LEFT_SHIFT A   &ctrlml LCTRL S  &kp D     &smhlayertapL 4 F  &kp G                                              &kp H                   &kp J         &kp K         &ctrlmr RCTRL L  &shmr RIGHT_SHIFT DE_O_UMLAUT  &shmr RIGHT_SHIFT DE_A_UMLAUT
&kp LCTRL       &shml LEFT_ALT DE_Y  &kp X            &kp C     &kp V              &smhlayertapFull 6 B  &none           &none        &smhlayertapFull 6 N    &kp M         &kp COMMA     &kp PERIOD       &shmr LEFT_ALT DE_MINUS        &shmr RIGHT_CONTROL DE_UNDERSCORE
                                     &kp LGUI         &kp LALT  &lt 3 ESCAPE       &kp SPACE             &lt 5 ESCAPE    &lt 5 ENTER  &mo 4                   &lt 3 ESCAPE  &kp LEFT_ALT  &kp RALT
            >;

            sensor-bindings =
                <&inc_dec_kp C_VOL_UP C_VOL_DN>,
                <&inc_dec_kp F21 F20>;
        };

        Siggboy_Layer {
            display-name = "siggboy";
            bindings = <
&kp ESC         &kp N1               &kp N2    &kp N3    &kp N4              &kp N5                                  &kp N6                &kp N7             &kp N8        &kp N9    &kp N0                 &kp MINUS
&kp TAB         &kp V                &kp G     &kp L     &mod_thorn_key 0 0  &kp DE_MINUS                            &kp DE_U_UMLAUT       &kp U              &kp O         &kp P     &mod_taptap DE_Q DE_Z  &kp DE_A_UMLAUT
&kp LEFT_SHIFT  &shml LEFT_SHIFT C   &kp S     &kp N     &smhlayertapL 4 T   &kp M                                   &kp K                 &kp I              &kp E         &kp A     &shmr RIGHT_SHIFT H    &shmr RIGHT_SHIFT DE_O_UMLAUT
&kp LCTRL       &shml LEFT_ALT DE_X  &kp F     &kp W     &kp DE_D            &smhlayertapFull 6 B  &trans    &trans  &smhlayertapFull 6 J  &mod_y_you 0 DE_Y  &kp COMMA     &kp DOT   &shmr LEFT_ALT DE_SZ   &shmr RIGHT_CONTROL DE_UNDERSCORE
                                     &kp LGUI  &kp LALT  &trans              &trans                &trans    &trans  &lt 4 R               &trans             &kp LEFT_ALT  &kp RALT
            >;

            sensor-bindings =
                <&inc_dec_kp C_VOL_UP C_VOL_DN>,
                <&inc_dec_kp F21 F20>;
        };

        Game_Layer {
            display-name = "game";
            bindings = <
&kp ESC         &kp N1     &kp N2        &kp N3    &kp N4        &kp N5                       &kp N6  &kp N7         &kp N8   &kp N9     &kp N0   &kp MINUS
&kp TAB         &kp NUBS   &kp Q         &kp W     &kp E         &kp R                        &kp T   &kp Y          &kp U    &kp I      &kp O    &kp P
&kp LEFT_SHIFT  &kp LSHFT  &kp A         &kp S     &kp D         &kp F                        &kp G   &kp H          &kp J    &kp K      &kp L    &mt RIGHT_SHIFT APOS
&kp LCTRL       &kp LCTRL  &kp Z         &kp X     &kp C         &kp V      &trans    &trans  &kp B   &kp N          &kp M    &kp COMMA  &kp DOT  &kp MINUS
                           &kp LEFT_GUI  &kp LALT  &kp LEFT_ALT  &kp SPACE  &trans    &trans  &mo 3   &kp BACKSPACE  &kp DEL  &kp RALT
            >;

            sensor-bindings =
                <&inc_dec_kp C_VOL_UP C_VOL_DN>,
                <&inc_dec_kp F21 F20>;
        };

        Numpad_Layer {
            display-name = "numpad";
            bindings = <
&kp ESCAPE        &none                    &none                    &none                    &none                    &none                            &kp KP_NUMLOCK    &none                        &kp KP_DIVIDE    &kp KP_MULTIPLY  &kp DE_GRAVE               &kp DE_ACUTE
&kp TAB           &kp F13                  &kp F14                  &kp F15                  &kp F16                  &kp F17                          &kp DE_COLON      &kp KP_NUMBER_7              &kp KP_NUMBER_8  &kp KP_NUMBER_9  &kp DE_MINUS               &kp DE_HASH
&kp LSHFT         &mod_taptap LS(F18) F18  &mod_taptap LS(F19) F19  &mod_taptap LS(F20) F20  &mod_taptap LS(F21) F21  &kp F22                          &kp DE_SEMICOLON  &kp KP_NUMBER_4              &kp KP_NUMBER_5  &kp KP_NUMBER_6  &shmr RIGHT_SHIFT DE_PLUS  &kp LS(RIGHT_BRACKET)
&kp LEFT_CONTROL  &none                    &none                    &none                    &none                    &none    &trans    &trans        &kp KP_NUMBER_0   &tap_isEqual1 0 KP_NUMBER_1  &kp KP_NUMBER_2  &kp KP_NUMBER_3  &kp DE_EQUAL               &trans
                                           &trans                   &trans                   &trans                   &trans   &trans    &kp KP_ENTER  &kp SPACE         &tap_isEqual0 0 KP_NUMBER_0  &kp COMMA        &kp PERIOD
            >;

            sensor-bindings =
                <&inc_dec_kp C_VOL_UP C_VOL_DN>,
